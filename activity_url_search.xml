<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".UrlSearch" android:background="@android:color/holo_blue_dark">

    <EditText
            android:layout_width="wrap_content"
            android:layout_height="71dp"
            android:inputType="textMultiLine"
            android:gravity="start|top"
            android:ems="10"
            android:hint="Search or enter url"
            android:background="#A0A0D8"
            android:id="@+id/input_search_url" android:layout_alignRight="@+id/home_button"
            android:layout_marginRight="198dp"
            android:layout_marginEnd="198dp" android:layout_alignEnd="@+id/home_button"
            android:layout_alignParentBottom="true" android:layout_marginBottom="588dp" android:textSize="24sp"/><![CDATA[

    ]]><Button
            android:text="Search"
            android:layout_width="105dp"
            android:layout_height="62dp" tools:layout_editor_absoluteY="4dp"
            tools:layout_editor_absoluteX="204dp" android:id="@+id/search_url_button" tools:ignore="MissingConstraints"
            android:textSize="18sp" android:textColor="@android:color/background_dark"
            android:textColorHighlight="#000A0909" android:background="@android:color/holo_purple"
            android:layout_marginRight="-3dp"
            android:layout_marginEnd="-3dp"
            android:layout_alignParentBottom="true" android:layout_marginBottom="597dp"
            android:layout_toStartOf="@+id/home_button" android:layout_toLeftOf="@+id/home_button"/>
    <Button
            android:text="Home"
            android:layout_width="96dp"
            android:layout_height="58dp" tools:layout_editor_absoluteY="4dp"
            tools:layout_editor_absoluteX="312dp" android:id="@+id/home_button" android:textSize="18sp"
            android:background="@android:color/holo_red_light" android:layout_marginBottom="597dp"
            android:layout_alignParentBottom="true"
            android:layout_marginRight="0dp" android:layout_marginEnd="0dp"
            tools:ignore="MissingConstraints,UnknownId" android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true" />
    <WebView
            android:layout_width="match_parent"
            android:layout_height="595dp" android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:id="@+id/search_web_site"
            android:layout_marginRight="0dp" android:layout_marginEnd="0dp" android:layout_alignParentBottom="true"
            android:layout_marginBottom="0dp" tools:ignore="DuplicateIds" android:background="#D33E0F"/>


</RelativeLayout>